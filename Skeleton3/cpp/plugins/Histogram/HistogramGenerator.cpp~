#include "HistogramGenerator.h"
#include "qwt_plot.h"
HistogramGenerator::HistogramGenerator( QWidget *parent=0, char *name=0, double xValues[], double yValues[] ) : QwtPlot( parent, name )
  {
    setTitle( "Histogram" );
    setAxisTitle( xBottom, "x" );
    setAxisTitle( yLeft, "y" );

    long hist = insertCurve( "Histogram" );
    int points = xValues.size();
    QwtPlot* plot = new QwtPlot(0);
    setCurveData( hist, xValues, yValues, points );
    setCurvePen( hist, QPen( blue ) );
  }
